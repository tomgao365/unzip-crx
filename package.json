{
  "name": "@tomjs/unzip-crx",
  "version": "0.0.0",
  "description": "unzip chrome extension files",
  "keywords": [
    "tomjs",
    "node",
    "back-end",
    "backend",
    "unzip-crx",
    "unzip",
    "crx"
  ],
  "author": {
    "name": "Tom Gao",
    "email": "tom@tomgao.cc"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomgao365/unzip-crx.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "start": "tsx ./src/index.ts",
    "build": "tsup",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint \"src/**/*.ts\" *.{js,cjs,ts} --fix --cache",
    "lint:prettier": "prettier --write .",
    "prepare": "husky install",
    "prepublishOnly": "npm run build && np --any-branch --no-yarn --yolo --no-publish --message \"chore: release v%s\""
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@tomjs/commitlint": "^2.0.3",
    "@tomjs/eslint": "^1.0.8",
    "@tomjs/prettier": "^1.0.5",
    "@tomjs/tsconfig": "^1.0.7",
    "@types/node": "^18.18.13",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "np": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.6.0",
    "typescript": "~5.2.2"
  }
}
